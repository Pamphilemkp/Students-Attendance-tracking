<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
      <%= link_to 'Ban User', ban_user_path(user), method: :patch %>

    </p>
  <% end %>
</div>


<%= link_to "New user", new_user_path %>
